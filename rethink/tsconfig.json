{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es5",
		"module": "system",
		"moduleResolution": "node",
		"removeComments": true,
		"preserveConstEnums": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist"
	},
	"filesGlob": [
		"main.ts",
		"app/**/*.ts",
		"comp/**/*.ts",
		"services/**/*.ts",
		"typings/**/*.ts"
	],
	"files": [
		"main.ts",
		"app/activityView.ts",
		"app/app.ts",
		"app/userView.ts",
		"comp/activity/activity.comp.ts",
		"comp/activity/activity.ts",
		"comp/activity/activitylist.comp.ts",
		"comp/contact/contact.comp.ts",
		"comp/contact/contact.ts",
		"comp/contact/contactlist.comp.ts",
		"comp/contact/me.comp.ts",
		"comp/context/breadcrumb.comp.ts",
		"comp/context/context.ts",
		"comp/context/menu.comp.ts",
		"comp/context/sender.comp.ts",
		"comp/fileshare/filesharelist.comp.ts",
		"comp/user/contact-box.comp.ts",
		"comp/user/content-box.comp.ts",
		"comp/user/notification.comp.ts",
		"comp/user/video-contact-box.comp.ts",
		"services/app.service.ts",
		"services/chat.service.ts",
		"services/contacts.ts",
		"services/context.service.ts",
		"services/me.ts",
		"services/modules.d.ts",
		"services/video.service.ts",
		"typings/globals/es6-shim/index.d.ts",
		"typings/globals/jquery/index.d.ts",
		"typings/globals/mediastream/index.d.ts",
		"typings/globals/rtcpeerconnection/index.d.ts",
		"typings/index.d.ts"
	],
	"atom": {
		"rewriteTsconfig": true
	}
}
