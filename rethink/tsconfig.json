{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es5",
		"module": "system",
		"moduleResolution": "node",
		"removeComments": true,
		"preserveConstEnums": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist"
	},
	"filesGlob": [
		"main.ts",
		"application/**/*.ts",
		"components/**/*.ts",
		"services/**/*.ts",
		"models/**/*.ts",
		"typings/**/*.ts"
	],
	"files": [
		"main.ts",
		"components/rethink/communication/Communication.ts",
		"components/rethink/context/Context.ts",
		"components/rethink/hypertyResource/audio/audio.component.ts",
		"components/rethink/hypertyResource/chat/chat.component.ts",
		"components/rethink/hypertyResource/chat/chatEvent.component.ts",
		"components/rethink/hypertyResource/file/file.component.ts",
		"components/rethink/hypertyResource/file/fileEvent.component.ts",
		"components/rethink/hypertyResource/video/video.component.ts",
		"components/rethink/userIdentity/UserIdentity.ts",
		"services/app.service.ts",
		"services/chat.service.ts",
		"services/contact.service.ts",
		"services/context.service.ts",
		"services/message.service.ts",
		"services/modules.d.ts",
		"services/services.ts",
		"services/storage.service.ts",
		"services/video.service.ts",
		"models/application/Context.ts",
		"models/application/ContextualComm.ts",
		"models/application/ContextualCommTrigger.ts",
		"models/models.ts",
		"models/rethink/Communication.ts",
		"models/rethink/Connection.ts",
		"models/rethink/Context.ts",
		"models/rethink/HypertyResource.ts",
		"models/rethink/UserIdentity.ts",
		"typings/globals/es6-shim/index.d.ts",
		"typings/globals/jquery/index.d.ts",
		"typings/globals/mediastream/index.d.ts",
		"typings/globals/rtcpeerconnection/index.d.ts",
		"typings/index.d.ts"
	],
	"atom": {
		"rewriteTsconfig": true
	}
}
