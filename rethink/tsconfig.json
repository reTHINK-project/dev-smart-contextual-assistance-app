{
	"compileOnSave": true,
	"compilerOptions": {
		"target": "es5",
		"module": "system",
		"moduleResolution": "node",
		"removeComments": true,
		"preserveConstEnums": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist"
	},
	"filesGlob": [
		"main.ts",
		"app/**/*.ts",
		"components/**/*.ts",
		"services/**/*.ts",
		"models/**/*.ts",
		"typings/**/*.ts"
	],
	"files": [
		"main.ts",
		"app/activity/activity.component.ts",
		"app/breadcrumb/breadcrumb.component.ts",
		"app/compositeContextualComm/CompositeContextualComm.ts",
