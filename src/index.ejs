<!DOCTYPE html>
<html lang="en" class="h-100">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">


    <!-- On github put: /dev-smart-contextual-assistance-app/ -->
    <base href="<%= htmlWebpackPlugin.options.baseHref %>">

    <title><%= htmlWebpackPlugin.options.title || 'Webpack App'%></title>

    <link rel="canonical" href="./index.html">

    <link rel="apple-touch-icon" type="image/png" sizes="57x57" href="./assets/img/icons/apple-icon-57x57.png">
    <link rel="apple-touch-icon" type="image/png" sizes="60x60" href="./assets/img/icons/apple-icon-60x60.png">
    <link rel="apple-touch-icon" type="image/png" sizes="72x72" href="./assets/img/icons/apple-icon-72x72.png">
    <link rel="apple-touch-icon" type="image/png" sizes="76x76" href="./assets/img/icons/apple-icon-76x76.png">
    <link rel="apple-touch-icon" type="image/png" sizes="114x114" href="./assets/img/icons/apple-icon-114x114.png">
    <link rel="apple-touch-icon" type="image/png" sizes="120x120" href="./assets/img/icons/apple-icon-120x120.png">
    <link rel="apple-touch-icon" type="image/png" sizes="144x144" href="./assets/img/icons/apple-icon-144x144.png">
    <link rel="apple-touch-icon" type="image/png" sizes="152x152" href="./assets/img/icons/apple-icon-152x152.png">
    <link rel="apple-touch-icon" type="image/png" sizes="180x180" href="./assets/img/icons/apple-icon-180x180.png">

    <link rel="icon" type="image/png" sizes="192x192" href="./assets/img/icons/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="./assets/img/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="./assets/img/icons/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="./assets/img/icons/favicon-16x16.png">

    <meta name="msapplication-TileColor" content="#364258">
    <meta name="msapplication-TileImage" content="./assets/img/icons/ms-icon-144x144.png">

    <meta name="theme-color" content="#364258" />
    <meta name="mobile-web-app-capable" content="yes"/>

    <link rel="manifest" href="./manifest.json" />

    <% if (htmlWebpackPlugin.files.favicon) { %>
    <link rel="shortcut icon" type="image/x-icon" href="<%= htmlWebpackPlugin.files.favicon %>">
    <% } %>

    <% if (htmlWebpackPlugin.options.mobile) { %>
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no, maximum-scale=1, minimum-scale=1">
    <% } %>

    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
    <% } %>

    <script>
      (function(){
        var redirect = sessionStorage.redirect;
        delete sessionStorage.redirect;
        if (redirect && redirect != location.href) {
          history.replaceState(null, null, redirect);
        }
      })();
    </script>

  </head>

  <body class="rethink work h-100">

    <rethink-app>
      <!-- Loading spinner when wait for the auth and app be ready -->
      <div  class="main-content d-flex flex-row align-items-center h-100">
        <div class="mx-auto text-center">
          <i class="fa fa-cog fa-spin fa-4x"></i>
        </div>
      </div>
    </rethink-app>


    <noscript>
        <p>
          Sorry, JavaScript must be enabled to use this app
        </p>
    </noscript>

  </body>
</html>
